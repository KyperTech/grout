{
  "parser": "babel-eslint",
  "rules": {
    "strict": 0,
    "quotes": [2, "single"],
    "no-unused-expressions": 0,
    "indent": [1,"tab"]
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "mocha": true
  },
  "globals": {
    "spy": true,
    "expect": true,
    "sinon": true
  },
  "plugins": [
    "react"
  ]
}
