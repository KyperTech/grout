{
  "name": "kyper-grout",
  "version": "0.1.2",
  "description": "Library to communicate with tessellate application building/managing service.",
  "main": "lib/index.js",
  "scripts": {
    "build:lib": "babel src --out-dir lib",
    "build:umd": "webpack src/index.js dist/grout.js --config webpack.config.development.js",
    "build:umd:min": "webpack src/index.js dist/grout.min.js --config webpack.config.production.js",
    "build": "npm run clean && npm run build:lib && npm run build:umd && npm run build:umd:min",
    "watch:umd": "npm run build:umd -- --stats --progress --colors --watch",
    "watch:lib": "npm run build:lib -- --stats --progress --colors --watch",
    "watch": "npm run watch:umd",
    "clean": "rimraf lib dist",
    "prepublish": "npm run clean && npm run build",
    "test": "mocha --compilers js:babel-core/register --reporter spec ./test/**/*.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kypertech/grout.git"
  },
  "files": [
    "src",
    "dist"
  ],
  "keywords": [
    "kyper",
    "grout",
    "matter",
    "tessellate",
    "authentication",
    "user management",
    "application management"
  ],
  "author": "Kyper LLC <kyper.dev@gmail.com>",
  "contributors": [
    {
      "name": "Scott Prue",
      "email": "<sprue.dev@gmail.com>"
    },
    {
      "name": "Mel van Londen",
      "email": "<melvanlonden@gmail.com>"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kypertech/grout/issues"
  },
  "homepage": "https://github.com/kypertech/grout",
  "dependencies": {
    "aws-sdk": "^2.2.26",
    "firebase": "^2.3.2",
    "kyper-matter": "0.1.7",
    "lodash": "^3.10.1",
    "qs": "^6.0.1",
    "superagent": "^1.4.0"
  },
  "devDependencies": {
    "babel": "^6.1.18",
    "babel-cli": "^6.2.0",
    "babel-core": "^6.2.1",
    "babel-eslint": "^5.0.0-beta4",
    "babel-loader": "^6.2.0",
    "babel-plugin-add-module-exports": "^0.1.1",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-stage-0": "^6.1.18",
    "brfs": "^1.4.1",
    "chai": "^3.2.0",
    "chai-as-promised": "^5.1.0",
    "gulp": "^3.8.10",
    "gulp-awspublish": "^3.0.0",
    "gulp-eslint": "^1.0.0",
    "gulp-file": "^0.2.0",
    "gulp-filter": "^3.0.0",
    "gulp-jscs": "^2.0.0",
    "gulp-jsdoc": "^0.1.4",
    "gulp-load-plugins": "^0.10.0",
    "gulp-notify": "^2.1.0",
    "gulp-plumber": "^1.0.1",
    "gulp-rename": "^1.2.0",
    "gulp-shell": "^0.4.3",
    "jsdoc": "^3.3.2",
    "json-loader": "^0.5.4",
    "mocha": "^2.2.5",
    "rimraf": "^2.4.4",
    "run-sequence": "^1.0.2",
    "sinon": "^1.12.2",
    "sinon-chai": "^2.7.0",
    "transform-loader": "^0.2.3",
    "webpack": "^1.12.9"
  }
}
